{
  "*.html": [
    "js-beautify -r"
  ],
  "*.scss": [
    "stylelint --syntax scss --fix",
    "prettier --write"
  ],
  "*.ts": [
    "tsc --experimentalDecorators --forceConsistentCasingInFileNames --noEmit --noFallthroughCasesInSwitch --noImplicitReturns --noUnusedLocals --noUnusedParameters  --strict",
    "eslint --fix"
  ]
}
