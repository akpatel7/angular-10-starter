{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "bundle:analyze": "source-map-explorer dist/**/*.js",
    "bundle:build": "ng build --prod --source-map",
    "clean": "concurrently \"rimraf coverage\" \"rimraf dist\" \"rimraf npm-debug.log*\" \"npm cache verify\" \"npm prune\"",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html",
    "lint:html": "js-beautify src/**/*.html -r",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint src/**/*.scss --syntax scss --fix",
    "postinstall": "patch-package --patch-dir scripts/patches",
    "start": "ng serve",
    "start:build": "ws --spa index.html --directory dist --hostname localhost --port 1337",
    "test": "ng test --verbose"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/forms": "11.0.5",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.1",
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular-eslint/builder": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.6",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.6",
    "@angular-eslint/schematics": "0.8.0-beta.6",
    "@angular-eslint/template-parser": "0.8.0-beta.6",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ngneat/spectator": "6.1.2",
    "@types/jest": "26.0.19",
    "@types/jest-when": "2.7.2",
    "@types/node": "14.14.14",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "concurrently": "5.3.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-no-use-extend-native": "0.5.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unicorn": "24.0.0",
    "eslint-plugin-unused-imports": "1.0.1",
    "husky": "4.3.6",
    "jasmine-marbles": "0.6.0",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "jest-junit": "12.0.0",
    "jest-preset-angular": "8.3.2",
    "jest-when": "3.0.1",
    "js-beautify": "1.13.0",
    "lint-staged": "10.5.3",
    "local-web-server": "4.2.1",
    "mockdate": "3.0.2",
    "ng-mocks": "11.2.6",
    "patch-package": "6.2.2",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "source-map-explorer": "2.5.1",
    "stylelint": "13.8.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-no-unsupported-browser-features": "4.1.4",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "stylelint-use-nesting": "3.0.0",
    "stylelint-z-index-value-constraint": "1.1.0",
    "typescript": "4.0.2"
  }
}
