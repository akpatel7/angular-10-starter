{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "bundle:analyze": "source-map-explorer dist/**/*.js",
    "bundle:build": "ng build --prod --source-map",
    "clean": "concurrently \"rimraf coverage\" \"rimraf dist\" \"rimraf npm-debug.log*\" \"npm cache verify\" \"npm prune\"",
    "lint": "npm run type-check && npm run lint:ts && npm run lint:scss && npm run lint:html && echo Linted all files successfully!",
    "lint:html": "js-beautify src/**/*.html -r",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint src/**/*.scss --syntax scss --fix",
    "postinstall": "patch-package --patch-dir scripts/patches",
    "start": "ng serve",
    "start:build": "ws --spa index.html --directory dist --hostname localhost --port 1337",
    "test": "jest --verbose --coverage",
    "test:ci": "jest --coverage",
    "type-check": "tsc -p tsconfig.typecheck.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.3",
    "@angular/common": "10.1.3",
    "@angular/compiler": "10.1.3",
    "@angular/core": "10.1.3",
    "@angular/forms": "10.1.3",
    "@angular/platform-browser": "10.1.3",
    "@angular/platform-browser-dynamic": "10.1.3",
    "@angular/router": "10.1.3",
    "rxjs": "6.6.3",
    "tslib": "2.0.1",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.1",
    "@angular-devkit/build-angular": "0.1001.3",
    "@angular-eslint/builder": "0.4.0-beta.2",
    "@angular-eslint/eslint-plugin": "0.4.0-beta.2",
    "@angular-eslint/eslint-plugin-template": "0.4.0-beta.2",
    "@angular-eslint/schematics": "0.4.0-beta.2",
    "@angular-eslint/template-parser": "0.4.0-beta.2",
    "@angular/cli": "10.1.3",
    "@angular/compiler-cli": "10.1.3",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@ngneat/spectator": "5.13.3",
    "@types/jest": "26.0.14",
    "@types/jest-when": "2.7.1",
    "@types/node": "14.11.2",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "concurrently": "5.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-html": "6.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.0.2",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-unicorn": "22.0.0",
    "eslint-plugin-unused-imports": "0.1.3",
    "husky": "4.3.0",
    "jasmine-marbles": "0.6.0",
    "jest": "26.4.2",
    "jest-extended": "0.11.5",
    "jest-junit": "11.1.0",
    "jest-preset-angular": "8.3.1",
    "jest-when": "2.7.2",
    "js-beautify": "1.13.0",
    "lint-staged": "10.4.0",
    "local-web-server": "4.2.1",
    "mockdate": "3.0.2",
    "ng-mocks": "10.1.3",
    "patch-package": "6.2.2",
    "prettier": "2.1.2",
    "prettier-eslint": "11.0.0",
    "source-map-explorer": "2.5.0",
    "stylelint": "13.7.2",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-no-unsupported-browser-features": "4.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "stylelint-use-nesting": "3.0.0",
    "stylelint-z-index-value-constraint": "1.1.0",
    "typescript": "4.0.3"
  },
  "jest-junit": {
    "outputDirectory": "./junit"
  }
}
