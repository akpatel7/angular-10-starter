{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "bundle:analyze": "source-map-explorer dist/**/*.js",
    "bundle:build": "ng build --prod --source-map",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html && echo Linted all files successfully!",
    "lint:html": "js-beautify src/**/*.html -r",
    "lint:ts": "ng lint",
    "lint:scss": "stylelint src/**/*.scss --syntax scss --cache --fix",
    "start": "ng serve",
    "start:build": "ws --spa index.html --directory dist --hostname localhost --port 1337",
    "test": "jest --verbose --coverage",
    "test:ci": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.0.14",
    "@angular/common": "10.0.14",
    "@angular/compiler": "10.0.14",
    "@angular/core": "10.0.14",
    "@angular/forms": "10.0.14",
    "@angular/platform-browser": "10.0.14",
    "@angular/platform-browser-dynamic": "10.0.14",
    "@angular/router": "10.0.14",
    "rxjs": "6.6.2",
    "tslib": "2.0.1",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.0",
    "@angular-devkit/build-angular": "0.1000.8",
    "@angular-eslint/builder": "0.2.0-beta.1",
    "@angular-eslint/eslint-plugin": "0.2.0-beta.1",
    "@angular-eslint/eslint-plugin-template": "0.2.0-beta.1",
    "@angular-eslint/schematics": "0.2.0-beta.1",
    "@angular-eslint/template-parser": "0.2.0-beta.1",
    "@angular/cli": "10.0.8",
    "@angular/compiler-cli": "10.0.14",
    "@ngneat/spectator": "5.13.1",
    "@types/jest": "26.0.12",
    "@types/jest-when": "2.7.1",
    "@types/node": "14.6.2",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-html": "6.0.3",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-unicorn": "21.0.0",
    "husky": "4.2.5",
    "jasmine-marbles": "0.6.0",
    "jest": "26.4.2",
    "jest-extended": "0.11.5",
    "jest-junit": "11.1.0",
    "jest-preset-angular": "8.3.1",
    "jest-when": "2.7.2",
    "js-beautify": "1.13.0",
    "lint-staged": "10.2.13",
    "local-web-server": "4.2.1",
    "mockdate": "3.0.2",
    "ng-mocks": "10.1.2",
    "prettier": "2.1.1",
    "prettier-eslint": "11.0.0",
    "source-map-explorer": "2.5.0",
    "stylelint": "13.7.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-no-unsupported-browser-features": "4.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.1.2",
